{
  "extends": "stylelint-config-recommended",
  "plugins": [
    "stylelint-scss"
  ],
  "rules": {
    "at-rule-no-unknown": null,
    "scss/at-rule-no-unknown": true,
    "indentation": [
      2,
      {
        "severity": "warning"
      }
    ],
    "linebreaks": [
      "unix",
      {
        "severity": "warning"
      }
    ],
    "no-eol-whitespace": true,
    "no-missing-end-of-source-newline": true
  }}
